<%= node['rails_env'] %>:
<% node['services_config'].keys.each do |s| %>
  <% if node['services_config'][s].is_a?(String) %>
  <%=  s %>: <%= node['services_config'][s] %>
  <% else %>
  <%= s %>:
  <% node['services_config'][s].each do |k, v| %>
    <%= k %>: <%= v %>
  <% end %>
<% end %>
<% end %>
